<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<!-- アクションのパッケージ定義 -->
	<package name="default" extends="struts-default">
		<!-- Hello worldのサンプルアクション -->
		<action name="hello" class="actions.HelloWorldAction" method="execute">
			<result name="success">/view/HelloWorld.jsp</result>
		</action>
		<action name="list" class="actions.DisplayListAction" method="execute">
			<result name="success">/view/list.jsp</result>
		</action>
		<action name="confirm" class="actions.DisplayConfirmAction" method="execute">
			<result name="success">/view/confirm.jsp</result>
		</action>
	</package>
</struts>
